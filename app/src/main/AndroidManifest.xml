<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.j4ftech.music.transpositiontrainer">
    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.j4ftech.music.transpositiontrainer.MainActivity"
            android:label="@string/app_name"
            android:screenOrientation="portrait" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.j4ftech.music.transpositiontrainer.Game"
            android:label="@string/title_activity_game"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="com.j4ftech.music.transpositiontrainer.Faq"
            android:label="@string/faqActivityTitle"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="com.j4ftech.music.transpositiontrainer.StudyRoom"
            android:label="@string/studyNotesActivityTitle"
            android:screenOrientation="portrait" >
        </activity>
    </application>

</manifest>
